@using Final_Exam.Models
@model Friend
<div>
    <h1>Dojo Meet Up Center</h1>
</div>
<div class="text-right">
    <p><a class="btn mt-2 float-end" href="/Dashboard">Home</a></p>
    <p><a class="btn mt-2 float-end" href="/">Log Out</a></p>
</div>
<div class="text-center">
    <div class="row">
        <div class="col-12">
            <h2>@ViewBag.AllMeet_Ups.Title</h2>
            <h3>Event Cordinator: @ViewBag.AllMeet_Ups.Event_Coordinator.First_Name @ViewBag.AllMeet_Ups.Event_Coordinator.Last_Name</h3>
            <h3>Description: @ViewBag.AllMeet_Ups.Description</h3>
            <ul>Participants:
                @foreach (Friend l in @ViewBag.Friends)
                {
                <li>@l.User.First_Name @l.User.Last_Name</li>
                }
            </ul>
        </div>
    </div>
</div>
